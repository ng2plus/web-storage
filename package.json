    "@angular/platform-browser-dynamic": "2.0.0",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.40",
    "@types/webpack": "^1.12.33",
    "awesome-typescript-loader": "^2.2.1",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
