{
  "name": "@ng2plus/web-storage",
  "version": "0.3.0",
  "main": "index.js",
  "description": "Ultimate web storage for Angular2",
  "keywords": [
    "angular2",
    "provider",
    "localstorage",
    "sessionstorage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2plus/web-storage.git"
  },
  "license": "MIT",
  "scripts": {
    "flow.clean": "node node_modules/del-cli/cli \"src/**/*.+(js|d.ts|js.map)\" \"index.+(js|d.ts|js.map)\"",
